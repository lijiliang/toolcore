{
  "name": "toolcore",
  "version": "0.2.3",
  "description": "js 常用前端代码工具库",
  "source": "index.js",
  "main": "dist/toolcore.common.js",
  "module": "dist/toolcore.esm.js",
  "unpkg": "dist/toolcore.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel index.js --no-source-maps",
    "build": "parcel build index.js --global toolcore --no-source-maps --experimental-scope-hoisting --detailed-report",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build:dist": "node build/build.js",
    "build:test": "rollup index.js --file bundle.js --format umd --name 'toolcore'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lijiliang/toolcore.git"
  },
  "keywords": [
    "toolcore",
    "core",
    "common",
    "util",
    "utils",
    "lib",
    "libs",
    "javascript"
  ],
